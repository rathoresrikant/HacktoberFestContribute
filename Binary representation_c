#include<stdio.h>
int main(){
   int t,i=1;
   scanf("%d",&t);
   while(i<=t){
       int num,r,s=0,k=1;
       scanf("%d",&num);
       while(num!=0){
           r=num%2;
           s=s+r*k;
           num=num/2;
           k*=10;
       }
       printf("%d\n",s);
        i++;
   }  
  
    
    return 0;
}

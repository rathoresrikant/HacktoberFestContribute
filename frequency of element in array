#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;  
    cout<<"Enter number of elements in array: ";    cin>>n;
    int arr[n];
    for(int i=0;i<n;i++)    cin>>arr[i];
    int test;
    cout<<"\nEnter the element you want to search: ";
    cin>>test;
    int count=0;
    for(int i=0;i<n;i++)
    {
        if(test==arr[i])    count++;
    }
    if(count==0)    cout<<"\nElement does not occur :(\n";
    else            cout<<"\nElement occurs "<<count<<" time(s)";
    return  0;
}

#include <bits/stdc++.h>
using namespace std;

int main() {
	int n,a,b;cin>>n;
	while(n--){
	    cin>>a>>b;
	    if(a>b){swap(a,b);}
	    if(abs(a-b)==1 and (a%2!=0) and (b%2==0)){cout<<"YES"<<endl;}
	    else if((abs(a-b)==2) and (a%2!=0) == (b%2!=0)){cout<<"YES"<<endl;}
	    else{cout<<"NO"<<endl;}
	}
	return 0;
}
